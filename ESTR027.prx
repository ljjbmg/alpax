*/
Static Function R460STrib(cProduto)
Local aAreaAnt := GetArea()
Local aAreaSB1 := SB1->(GetArea())
Local aAreaSD1 := SD1->(GetArea())
Local aAreaSD2 := SD2->(GetArea())
Local cSitTrib := ''
Local lContinua:= .T.

dbSelectArea('SB1')
dbSetOrder(1)
If dbSeek(xFilial('SB1')+cProduto) .And. !Empty(SB1->B1_ORIGEM)
	cSitTrib := AllTrim(SB1->B1_ORIGEM)+"-"
	//здддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
	//Ё Analisa Situacao Tributaria atraves da TES de Entrada Padrao |
	//юдддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддды
	If !Empty(SB1->B1_TE)
		dbSelectArea('SF4')
		dbSetOrder(1)
		If dbSeek(xFilial('SF4')+SB1->B1_TE) .And. !Empty(SF4->F4_SITTRIB)
			cSitTrib  := cSitTrib+AllTrim(SF4->F4_SITTRIB)
			lContinua := .F.
		EndIf
	//здддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
	//Ё Analisa Situacao Tributaria atraves da TES de Saida Padrao   |
	//юдддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддды
	ElseIf !Empty(SB1->B1_TS)
		dbSelectArea('SF4')
		dbSetOrder(1)
		If dbSeek(xFilial('SF4')+SB1->B1_TS) .And. !Empty(SF4->F4_SITTRIB)
			cSitTrib  := cSitTrib+AllTrim(SF4->F4_SITTRIB)
			lContinua := .F.
		EndIf
	EndIf
	//здддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
	//Ё Quando nao for cadastrada a TES padrao analisar os documentos|
	//| de Entrada/Saida.                                            |
	//юдддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддды
	If lContinua
		//здддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
		//Ё Analisa Situacao Tributaria atraves do Documento de Entrada  |
		//юдддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддды
		dbSelectArea('SD1')
		dbSetOrder(2)
		dbSeek(xFilial('SD1')+cProduto+Replicate("z",Len(SD1->D1_DOC)),.T.)
		dbSkip(-1)
		dbSelectArea('SF4')
		dbSetOrder(1)
		If dbSeek(xFilial('SF4')+SD1->D1_TES) .And. !Empty(SF4->F4_SITTRIB)
			cSitTrib  := cSitTrib+AllTrim(SF4->F4_SITTRIB)
			lContinua := .F.
		EndIf
	EndIf
	//здддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
	//Ё Quando nao e localizada a situacao tributaria na TES informa |
	//| o codigo 90 - Outras.                                        |
	//юдддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддды
	If lContinua .And. Len(Alltrim(cSitTrib)) == 2
		cSitTrib := cSitTrib + '90'
	EndIf	
EndIf
RestArea(aAreaSD1)
RestArea(aAreaSD2)
RestArea(aAreaSB1)
RestArea(aAreaAnt)
Return IIf(Empty(cSitTrib),'0-90',cSitTrib)

