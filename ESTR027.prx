		cQuery += "ORDER BY 1,2,8"	//FILIAL+PRODUTO+LOCAL
		*/

		cQuery := "SELECT "
		cQuery += "'01' AS B1_FILIAL, "
		cQuery += "ZBA_COD AS B1_COD, "
		cQuery += "ZBA_TIPO AS B1_TIPO, "
